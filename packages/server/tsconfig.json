{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@shared/*": ["./src/_shared/*"],
      "@config/*": ["./src/config/*"],
      "@modules/*": ["./src/modules/*"],
      "@utils/*": ["./src/_shared/utils/*"]
    }, 
  },  
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./*",
    "./src/**/*"
  ]
}